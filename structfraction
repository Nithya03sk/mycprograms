#include<stdio.h>
struct fraction
{
    int numerator;
    int denominator;
    
};
struct fraction input()
{
    struct fraction f;
    printf("enter fraction \n");
    scanf("%d%d",&f.numerator,&f.denominator);
    return f;
}
int lcm(int a,int b)
{
int temp=1;
if(temp%a==0&&temp%b==0)
return temp;
temp++;
lcm(a,b);
}
Void add(int a,int b)
{
  p=f1.numerator+f2.numerator;
  return p;
}
int output(int p,int l)
{
Printf("addition of fraction is",p/l);
}



int main()
{
    struct  fraction f1=input();
    struct fraction f2=input();
    printf("enter fraction 1");
    f1=input(&f1.numerator,&f1.denominator);
    printf("enter fraction 2");
    f2=input(&f2.numerator,&f2.denominator);
    l=lcm(f1.denomimator,f2.denominator);
    a=add(f1.numerator,f2.numerator);
    output(l,a);
}
